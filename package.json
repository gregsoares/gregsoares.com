    "core-js": "^3.46.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.5",
    "react-toastify": "^11.0.5",
    "styled-components": "^6.1.12",
